{
  "name": "Fantasy Sports Bar",
  "description": "View fantasy sports stats for your league during broadcasts",
  "version": "1.0",
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content-script.js"],
    "run_at": "document_end"
  }],
  "web_accessible_resources": [
    {
      "resources": ["index.html", "componentDist/*"],
      "matches": ["<all_urls>"]
    }
  ],
  "manifest_version": 3
}
